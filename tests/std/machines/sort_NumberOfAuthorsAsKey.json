    "csl": "<style \n      xmlns=\"http://purl.org/net/xbiblio/csl\"\n      class=\"in-text\"\n      version=\"1.0\">\n  <info>\n    <id />\n    <title />\n    <updated>2009-08-10T04:49:00+09:00</updated>\n  </info>\n  <macro name=\"author-one\">\n    <names variable=\"author\">\n      <name name-as-sort-order=\"first\" />\n      <substitute>\n        <names variable=\"editor\" />\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-count\">\n    <names variable=\"author\">\n      <name form=\"count\" />\n      <substitute>\n        <names variable=\"editor\" />\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"theyear\">\n    <date variable=\"issued\">\n      <date-part name=\"year\" />\n    </date>\n  </macro>\n  <citation>\n    <layout>\n      <text value=\"Ignore me\" />\n    </layout>\n  </citation>\n  <bibliography>\n    <sort>\n      <key macro=\"author-one\" names-min=\"1\" names-use-first=\"1\" />\n      <key macro=\"author-count\" names-min=\"3\" names-use-first=\"3\" />\n      <key macro=\"theyear\" />\n    </sort>\n    <layout>\n      <text variable=\"title\" />\n    </layout>\n  </bibliography>\n</style>", 
